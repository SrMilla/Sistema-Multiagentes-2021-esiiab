    <xs:include schemaLocation="Infomensaje.xsd"/>

    <xs:element name="msst">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="infoMensaje" type="infoMensaje"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
</xs:schema>